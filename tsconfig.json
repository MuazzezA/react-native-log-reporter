{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es2019",
    "module": "commonjs",
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "rootDir": "./"
  },
  "include": ["src"],
}
